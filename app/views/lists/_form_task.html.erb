<%= form_for [@list, @list.tasks.build] do |f| %>
			<p>ToDo名</p>
			<div class="field">
		    <%= f.text_field :title, class: 'form-control' %>
		  </div>
			<p>期限</p>
	    <div class="field">
	    	<%= f.date_select :deadline, class: 'form-control' %>
	    </div>
	    <%= f.submit "ToDoの追加", class: 'btn btn-primary' %>
<% end %>
